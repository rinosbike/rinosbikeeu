        },
        "collapsible_row_3UDbaw": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Do you offer a crash-replacement discount outside warranty?",
            "icon": "question_mark",
            "row_content": "<p>Yes we have a crash replacement program, please contact us at <a href=\"mailto:info@rinosbike.eu\"><span style=\"text-decoration:underline\">info@rinosbike.eu</span></a> if you have a crash and need parts.</p>",
            "page": ""
